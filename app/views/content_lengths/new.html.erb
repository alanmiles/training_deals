<% provide(:title, 'New content length') %>
<h1>New content length</h1>

<div class="row">
	<aside class="span3">
		<section>
			<%= render "/shared/admin_menu_side" %>
		</section>
	</aside>
	<div class="span8">
		<%= form_for(@length) do |f| %>

		 	<%= render 'fields', f: f %>

			<%= f.submit "Create", class: "btn btn-primary" %>
		<% end %>
	</div>
	<div class="navigator">
		<%= link_to "<- All content lengths", content_lengths_path %>
	</div>
</div> 
